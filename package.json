{
  "name": "grammuelle",
  "version": "0.0.1",
  "description": "SASS for humans.",
  "main": "theme.css",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "npm run compile",
    "compile": "sass .",
    "inspect": "less __theme__.css",
    "watch": "sass --watch __basics__.scss __core__.scss __interface__.scss __stylebook__.scss src theme.scss",
    "docs": "styledocco -n grammuelle -o docs -s __docs__ --preprocessor \"scss --compass\" src",
    "view.docs": "open docs/index.html",
    "inspect.bower": "less bower.json",
    "hotfix": "git add .; git commit -m \"#hotfix: $(date)\"; git push -u origin master",
    "do.module": "source ./bin/do.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdfiles/grammuelle.git"
  },
  "keywords": [
    "sass"
  ],
  "author": "nerdfiles",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/nerdfiles/grammuelle/issues"
  },
  "homepage": "https://github.com/nerdfiles/grammuelle#readme",
  "devDependencies": {
    "styledocco": "^0.6.6"
  },
  "dependencies": {
    "lodash": "^4.16.4",
    "promise-defer": "^1.0.0",
    "tachyons-sass": "^4.5.5"
  }
}
